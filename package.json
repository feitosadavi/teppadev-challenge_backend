{
  "name": "cardapp-backend",
  "version": "1.0.0",
  "description": "Backend for cardapp",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec \"ts-node-dev -r tsconfig-paths/register src/main/server.ts\" -e ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --setupFiles dotenv/config",
    "test:clear": "npm test -- --clearCache",
    "test:coverage": "npm test -- --coverage",
    "test:unit": "npm test -- --watchAll -c jest.unit.config.js",
    "test:verbose": "npm test -- --watch --passWithNoTests --runInBand",
    "test:integration": "npm test -- --watch -c jest.integration.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "bcrypt": "^5.0.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "firebase-admin": "^11.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1"
  }
}
